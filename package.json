{
  "name": "febu",
  "version": "0.18.3",
  "description": "前端构建工具",
  "repository": {
    "type": "git",
    "url": "https://github.com/holyzfy/febu.git"
  },
  "author": "holyzfy <zhaofuyun202@gmail.com>",
  "license": "MIT",
  "keywords": [
    "febu",
    "automated frontend tools"
  ],
  "bugs": {
    "url": "https://github.com/holyzfy/febu/issues"
  },
  "homepage": "https://github.com/holyzfy/febu",
  "scripts": {
    "preinstall": "npm ls -g gulp-cli || npm install -g gulp-cli",
    "eslint": "files=`git diff --staged --name-only --diff-filter=ACMRTU | grep -ie '\\.js$'`; if [ \"${files:=empty}\" != \"empty\" ]; then ./node_modules/.bin/eslint -c .eslintrc.json $files; fi",
    "test": "./node_modules/.bin/tape 'test/*.js' | ./node_modules/.bin/tap-spec"
  },
  "dependencies": {
    "async": "~2.6.1",
    "colors": "~1.3.0",
    "config": "~3.0.0",
    "debug": "~4.1.0",
    "del": "~3.0.0",
    "eslint": "~6.7.0",
    "frep": "~0.2.3",
    "fs-extra": "~7.0.1",
    "gulp": "~4.0.0",
    "gulp-clean-css": "~4.0.0",
    "gulp-concat": "~2.6.1",
    "gulp-filter": "~5.1.0",
    "gulp-if": "~2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-plumber": "~1.2.0",
    "gulp-rev": "~9.0.0",
    "gulp-uglify": "~3.0.0",
    "gulp-uglify-es": "~1.0.4",
    "husky": "~1.2.0",
    "parse-gitignore": "0.5.1",
    "proxyquire": "~2.1.0",
    "requirejs": "~2.3.5",
    "sinon": "~7.1.1",
    "tap-spec": "~5.0.0",
    "tape": "~4.9.1",
    "through2": "~3.0.0",
    "underscore": "~1.9.1",
    "vinyl": "~2.2.0",
    "yargs": "~12.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  }
}
