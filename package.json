{
  "name": "febu",
  "version": "0.14.4",
  "description": "前端构建工具",
  "repository": {
    "type": "git",
    "url": "https://github.com/holyzfy/febu.git"
  },
  "author": "holyzfy <zhaofuyun202@gmail.com>",
  "license": "MIT",
  "keywords": [
    "febu",
    "automated frontend tools"
  ],
  "bugs": {
    "url": "https://github.com/holyzfy/febu/issues"
  },
  "homepage": "https://github.com/holyzfy/febu",
  "scripts": {
    "preinstall": "npm ls -g gulp-cli || npm install -g gulp-cli",
    "eslint": "files=`git diff --staged --name-only --diff-filter=ACMRTU | grep -ie '\\.js$'`; if [ \"${files:=empty}\" != \"empty\" ]; then ./node_modules/.bin/eslint -c .eslintrc.json $files; fi",
    "precommit": "npm run eslint",
    "test": "./node_modules/.bin/tape 'test/*.js' | ./node_modules/.bin/tap-spec"
  },
  "dependencies": {
    "async": "~2.6.0",
    "colors": "~1.1.2",
    "config": "~1.29.2",
    "debug": "~3.1.0",
    "del": "~3.0.0",
    "eslint": "~4.17.0",
    "frep": "~0.2.3",
    "fs-extra": "~5.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-clean-css": "~3.9.2",
    "gulp-concat": "~2.6.1",
    "gulp-filter": "~5.1.0",
    "gulp-if": "~2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "~1.2.0",
    "gulp-rev": "~8.1.1",
    "gulp-uglify": "~3.0.0",
    "husky": "~0.14.3",
    "parse-gitignore": "~0.4.0",
    "proxyquire": "~1.8.0",
    "requirejs": "~2.3.5",
    "sinon": "~4.2.0",
    "tap-spec": "~4.1.1",
    "tape": "~4.9.0",
    "through2": "~2.0.3",
    "underscore": "~1.8.3",
    "vinyl": "~2.1.0",
    "yargs": "~11.0.0"
  }
}
